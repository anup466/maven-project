pipeline
{
agent any
stages{
stage('scm checkout')
{
steps{
  git branch: 'master', url: 'https://github.com/ssjoshi123/maven-project'
 }
 }
 stage('Package')
 {
 steps
 {
   withMaven(jdk: 'localjdk-1.8', maven: 'local maven'){
		sh 'mvn package'}
		}
		}
 stage('docker Build')
 {
 steps{
 sh 'docker build -t ssjoshi/tomcat:0.0.1 .'
 }
 }
  
		}
		}
