pipeline
{
agent any
stages
{
stage('scm checkout')
{
steps{
git branch: 'master', url: 'https://github.com/ssjoshi123/maven-project'
}
}
stage('package')
		{
		steps{
		withMaven(jdk: 'localjdk', maven: 'localmaven'){
		sh 'mvn package'
		archiveArtifacts 'webapp/target/*.war'
                hygieiaArtifactPublishStep artifactDirectory: 'webapp/target', artifactGroup: 'prinpal.com', artifactName: '*.war', artifactVersion: '1.0-SNAPSHOT'
                hygieiaDeployPublishStep applicationName: 'Product', artifactDirectory: 'webapp/target', artifactGroup: 'prinpal.com', artifactName: '*.war', artifactVersion: '1.0-SNAPSHOT', buildStatus: 'InProgress', environmentName: 'Dev'
		}
		}
		}
}
}
