pipeline
{
agent any
stages
{
stage('scm checkout')
{
steps{
git branch: 'master', url: 'https://github.com/ssjoshi123/maven-project'
}
}
stage('parallel execution')
{
parallel
{
stage('package')
{
steps
{
withMaven(jdk: 'localjdk-1.8', maven: 'local maven'){
		sh 'mvn package'}
}
}
stage('compile')
{
steps{
withMaven(jdk: 'localjdk-1.8', maven: 'local maven'){
		sh 'mvn compile'}
		
}
}
}
failfast: false
}
}
}
